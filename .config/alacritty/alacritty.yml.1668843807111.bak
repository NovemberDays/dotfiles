window:
  decorations: none
env:
  TERM:
    xterm-256color
colors:
  # Default colors
  primary:
    background: '#0A0E14'
    foreground: '#B3B1AD'

  # Normal colors
  normal:
    black: '#01060E'
    red: '#EA6C73'
    green: '#91B362'
    yellow: '#F9AF4F'
    blue: '#53BDFA'
    magenta: '#FAE994'
    cyan: '#90E1C6'
    white: '#C7C7C7'

  # Bright colors
  bright:
    black: '#686868'
    red: '#F07178'
    green: '#C2D94C'
    yellow: '#FFB454'
    blue: '#59C2FF'
    magenta: '#FFEE99'
    cyan: '#95E6CB'
    white: '#FFFFFF'
theme: Ayu-Dark

font:
  normal:
    family: Ubuntu Mono Nerd Font
    style: Medium
  bold:
    family: Ubuntu Mono Nerd Font
    style: Bold 
  italic:
    family: Ubuntu Mono Nerd Font
    style: MediumItalic 
  bold_italic:
    family: Ubuntu Mono Nerd Font
    style: BoldItalic 
  size: 17
draw_bold_text_with_bright_colors: true

key_bindings:
  - { key: F11, action: ToggleFullscreen }
